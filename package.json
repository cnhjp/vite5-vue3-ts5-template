{
  "name": "vite5-vue3-ts5-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve:dev": "vite --mode development --open",
    "serve:prod": "vite --mode production --opem",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "clipboard": "^2.0.11",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.1",
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/mdi": "^1.1.64",
    "@unhead/vue": "^1.8.20",
    "@unocss/eslint-plugin": "0.59.0-beta.1",
    "@unocss/preset-icons": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.1",
    "sass": "^1.72.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.2.2",
    "unocss": "^0.58.6",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
